<?xml version="1.0" encoding="UTF-8"?>
<manifest>

    <remote  name="github" fetch="git://github.com/cubieboard2/" />
    <remote  name="rpspec" fetch="https://github.com/cheaven/" />
    <remote  name="rpcode" fetch="https://github.com/robopeak/" />
    <default revision="cb2-test" remote="github" sync-j="4" />

    <project name="linux-sunxi" path="linux-sunxi" revision="sunxi-3.3-cb2" />
    <project name="cubieboard2_tools" path="tools" remote="rpspec">
        <copyfile src="scripts/build_rp.sh" dest="build.sh" />
    </project>
    <project name="cubieboard2_rootfs" path="rootfs" remote="rpspec"/>
    <project name="rpusbdisp" path="rpusbdisp" remote="rpcode" revision="master" />
</manifest>
